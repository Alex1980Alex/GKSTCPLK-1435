		<field xsi:type="DataSetFieldField">
			<dataPath>ТипРегистрации</dataPath>
			<field>ТипРегистрации</field>
			<attributeUseRestriction>
				<field>true</field>
				<condition>true</condition>
				<group>true</group>
				<order>true</order>
